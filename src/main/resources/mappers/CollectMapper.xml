<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.canteen.mapper.CollectMapper">
    <insert id="add">
        insert into s_collect(food_id,customer_id) values(#{fId},#{cId})
    </insert>
    <delete id="delete">
        delete from s_collect where food_id=#{fId} and customer_id=#{cId}
    </delete>
</mapper>